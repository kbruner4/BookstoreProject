@{
}

@model IEnumerable<string>

    <div class="d-grid gap-2">
        <a asp-controller="Home" asp-action="Index" asp-route-pageNum ="" class="btn btn-outline-secondary">Home</a><br />

        @foreach (string c in Model)
        {
            <a asp-controller="Home" asp-action="Index" asp-route-projectType="@c" asp-route-pageNum ="1" class="btn @(ViewBag.SelectedType == c ? " btn-primary" : "btn-outline-secondary")">@c</a>
            <br />
         }
    </div>